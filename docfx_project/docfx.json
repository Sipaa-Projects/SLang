{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SLangRT/SLangRT.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../"
        }
      ],
      "dest": "API",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "API/**.yml",
          "API/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}